{
  "openapi": "3.0.2",
  "info": {
    "version": "1.0",
    "title": "SMS API"
  },
  "paths": {
    "/messages": {
      "post": {
        "operationId": "create_message",
        "x-mojo-name": "post_message",
        "x-mojo-to": "message#create",
        "summary": "Create sms messages for a customer",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Messages response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "messages": {
                      "type": "array",
                      "items": {
                        "type": "object"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "get": {
        "operationId": "message",
        "x-mojo-name": "get_message",
        "x-mojo-to": "message#index",
        "summary": "List all sms messages from a customer",
        "parameters": [
          {
            "in": "query",
            "name": "name",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Messages response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "messages": {
                      "type": "array",
                      "items": {
                        "type": "object"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "servers": [
    {
      "url": "/api/v1"
    }
  ]
}